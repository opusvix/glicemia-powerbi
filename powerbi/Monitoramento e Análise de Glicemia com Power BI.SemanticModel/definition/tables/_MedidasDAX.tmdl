	measure 'Media Glicemia Jantar' =
			
			AVERAGE ( fGlicemia[preJanta] )
		formatString: 0
		displayFolder: Médias por Refeição
		lineageTag: 918acc96-d28a-42a9-b1d4-5272248094ee

